{
  "manifest_version": 3,
  "name": "Zswitcher",
  "version": "1.0",
  "description": "A simple and secure proxy switcher for Chrome. Easily configure and switch between proxy servers with a clean interface.",
  "permissions": [
    "proxy",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "minimum_chrome_version": "88",
  "author": "Zeo",
  "homepage_url": "https://github.com/godzeo/Zswitcher",
  "offline_enabled": true
} 